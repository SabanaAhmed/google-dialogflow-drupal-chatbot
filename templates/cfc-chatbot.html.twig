<div id="body">
  <div id="chat-circle" class="btn btn-raised">
    <div id="chat-overlay"></div>
    <img src="{{ variables['chatbot_logo'] }}">
  </div>
  <div class="chat-box">
    <div class="chat-box-header">
      <span>{{ variables['chatbot_title'] }}</span>
      <span class="chat-box-toggle">&times;</span>
    </div>
    <div class="chat-box-body">
      <div id="chat-overlay"></div>
      <div class="chat-logs" >
      </div><!--chat-log -->
    </div>
    <div class="chat-input">
      <form id="chatbot-form">
        <input type="text" id="chat-input" class="chatbotQuery" placeholder="Hey, ask me something..."/>
        <button type="submit" class="chat-submit" id="chat-submit">ASK</button>
      </form>
    </div>
  </div>
</div>
